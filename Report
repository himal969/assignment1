    private void jButton8ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        String id = jTextField1.getText();
        String name = jTextField2.getText();
        String phonenumber = jTextField3.getText();
        String dept = jTextField4.getText();
        String batch = jTextField5.getText();
        
        try
        {
            FileWriter Writer = new FileWriter("HIMAL.txt",true);
            Writer.write(""+id+""+name+""+phonenumber+""+dept+""+batch);
            Writer.write(System.getProperty("line.separator"));
            Writer.close();
            JOptionPane.showMessageDialog(null, "Success");
            setVisible(false);
            new NewJFrame().setVisible(true);
            
        }
        catch(Exception e)
        {
            JOptionPane.showMessageDialog(null,"Error");
        }
    }                                        

    private void jButton7ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        String id2 = jTextField6.getText();
        String name2 = jTextField7.getText();
        String trimester2 = jTextField8.getText();
        String cgpa2 = jTextField9.getText();
        String email2 = jTextField10.getText();
        
        try
        {
            FileWriter Writer = new FileWriter("HIMAL.txt",true);
            Writer.write(""+id2+""+name2+""+trimester2+""+cgpa2+""+email2);
            Writer.write(System.getProperty("line.separator"));
            Writer.close();
            JOptionPane.showMessageDialog(null, "Success");
            setVisible(false);
            new NewJFrame().setVisible(true);
            
        }
        catch(Exception e)
        {
            JOptionPane.showMessageDialog(null,"Error");
        }
    }                                        

