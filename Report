    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        // code to load data info table
        // validation for not leave blank data
        if (jTextField1.getText().equals("")||jTextField2.getText().equals("")||jTextField3.getText().equals("")||jTextField4.getText().equals("")||jTextField5.getText().equals("")){
           // if any of three field left blank than show message
           JOptionPane.showMessageDialog(this, " Please Enter All Data !");
        }
        else{
            // if all data field than....
            // store enter data into String array ---data---
            String data[]= {jTextField1.getText(),jTextField2.getText(),jTextField3.getText(),jTextField4.getText(),jTextField5.getText()};
            DefaultTableModel tblModel = (DefaultTableModel)jTable1.getModel();
            // add string array data
            tblModel.addRow(data);// row added
            // successfully added message
            JOptionPane.showMessageDialog(this, " Add Data Successfully...!");
            jTextField1.setText("");
            jTextField2.setText("");
            jTextField3.setText("");
            jTextField4.setText("");
            jTextField5.setText("");
        }
    }                                        

    private void jTextField9ActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
    }                                           

    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        // code to load data info table
        // validation for not leave blank data
        if (jTextField6.getText().equals("")||jTextField7.getText().equals("")||jTextField8.getText().equals("")||jTextField9.getText().equals("")){
           // if any of three field left blank than show message
           JOptionPane.showMessageDialog(this, " Please Enter All Data !");
        }
        else{
            // if all data field than....
            // store enter data into String array ---data---
            String data[]= {jTextField6.getText(),jTextField7.getText(),jTextField8.getText(),jTextField9.getText()};
            DefaultTableModel tblModel = (DefaultTableModel)jTable2.getModel();
            // add string array data
            tblModel.addRow(data);// row added
            // successfully added message
            JOptionPane.showMessageDialog(this, " Add Data Successfully...!");
            jTextField6.setText("");
            jTextField7.setText("");
            jTextField8.setText("");
            jTextField9.setText("");
        }
    }                                        

