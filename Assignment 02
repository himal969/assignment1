import java.awt.Color;


public class SimpleCalculator extends javax.swing.JFrame {
    double num,result;
    int calculation;

    
    public SimpleCalculator() {
        initComponents();
    }
   
      
         public void Enter()
         {
             
         
      
        switch( calculation)
        {
              case 1:
              result=num + Double.parseDouble(jTextField1.getText());
              jTextField1.setText(Double.toString(result));
              break;
              case 2:
              result=num - Double.parseDouble(jTextField1.getText());
              jTextField1.setText(Double.toString(result));
              break;
              case 3:
              result=num * Double.parseDouble(jTextField1.getText());
              jTextField1.setText(Double.toString(result));
              break;
              case 4:
              result=num / Double.parseDouble(jTextField1.getText());
              jTextField1.setText(Double.toString(result));
              break;
        }
         }
     

 
